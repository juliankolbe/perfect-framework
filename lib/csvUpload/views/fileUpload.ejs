<!doctype html>

<html>
  <head>
    <meta charset="utf-8">
  </head>
  <body>

    <form name="uploadForm"
          action="http://localhost:8080/upload"
          method="post"
          enctype="multipart/form-data"
          >
      <div>
        <input type="file" id="csvFiles" name="csvFiles" multiple="multiple" accept=".csv"/>
      </div>
      <br>
      <div>
        <input type="submit" value="Upload">
      </div>
    </form>
  </body>
</html>
